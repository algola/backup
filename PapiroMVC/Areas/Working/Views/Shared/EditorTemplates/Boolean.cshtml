@model bool?

<div class="control-group">
    <div class="col-sm-3 control-label no-padding-right">
        @Html.LabelFor(model => model)
    </div>
    <div class="controls col-sm-9">
        @Html.CheckBox("", Model.GetValueOrDefault())
        @Html.ValidationMessageFor(model => model)
    </div>
</div>

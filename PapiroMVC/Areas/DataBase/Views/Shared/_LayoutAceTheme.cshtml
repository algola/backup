<!DOCTYPE html>

<html lang="it">

<head>

    <meta charset="utf-8" />

    <title>Dashboard - Ace Admin</title>

    <meta name="description" content="overview &amp; stats" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
 
    <script src="~/Scripts/jquery-2.0.3.min.js"></script>

    <!-- basic styles -->
    <link href="~/Content/acetheme/assets/css/bootstrap.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/acetheme/assets/css/font-awesome.min.css" />

    <!--[if IE 7]>
		  <link rel="stylesheet" href="~/Content/acetheme/assets/css/font-awesome-ie7.min.css" />
		<![endif]-->

    <!-- page specific plugin styles -->
    @RenderSection("specificPlugInStyle", required: false)


    <!-- fonts -->
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:400,300" />

    <!-- ace styles -->
    <link rel="stylesheet" href="~/Content/acetheme/assets/css/ace.min.css" />
    <link rel="stylesheet" href="~/Content/acetheme/assets/css/ace-rtl.min.css" />
    <link rel="stylesheet" href="~/Content/acetheme/assets/css/ace-skins.min.css" />

    <!--[if lte IE 8]>
		  <link rel="stylesheet" href="~/Content/acetheme/assets/css/ace-ie.min.css" />
		<![endif]-->

    <!-- inline styles related to this page -->
    @RenderSection("inlineStyle", required: false)
    <link rel="stylesheet" href="~/Content/acetheme/assets/css/ui.jqgrid.css" />
    <link rel="stylesheet" href="~/Content/custom-icon.css" />


    <!-- ace settings handler -->
    <script src="~/Content/acetheme/assets/js/ace-extra.min.js"></script>

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>

		<script src="~/Content/acetheme/assets/js/html5shiv.js"></script>
		<script src="~/Content/acetheme/assets/js/respond.min.js"></script>
		<![endif]-->
</head>

<body>

    <header>
        <nav>
            <div class="navbar navbar-default" id="navbar">
                <script type="text/javascript">
                    try { ace.settings.check('navbar', 'fixed') } catch (e) { }
                </script>

                <!-- section for TopBar -->
                @Html.Partial("_TopBarAceTheme")
            </div>
        </nav>
    </header>

    <div class="main-container" id="main-container">
        <script type="text/javascript">
            try { ace.settings.check('main-container', 'fixed') } catch (e) { }
        </script>

        <div class="main-container-inner">
            <a class="menu-toggler" id="menu-toggler" href="#">
                <span class="menu-text"></span>
            </a>

            <!-- renderizzo la toolbar a sx richiudibile (sidebar)-->
            @Html.Partial("_SideBarAceTheme")

            <div class="main-content">
                @RenderSection("breadcrumb", required:false)
                
                <div class="page-content">
                    @RenderSection("featured", required: false)
                    @RenderBody()
                </div>
            </div>

        </div>
        <!-- /.main-container-inner -->

        <a href="#" id="btn-scroll-up" class="btn-scroll-up btn btn-sm btn-inverse">
            <i class="icon-double-angle-up icon-only bigger-110"></i>
        </a>

    </div>
    <!-- /.main-container -->


    <!-- Le javascript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->

    <!--<script src="~/Scripts/bootstrap.min.js"></script>-->

    <!-- basic scripts -->

    <!--[if !IE]> -->
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js"></script>
    <!-- <![endif]-->

    <!--[if IE]>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
<![endif]-->

    <!--[if !IE]> -->
    <script type="text/javascript">
        window.jQuery || document.write("<script src='~/Content/acetheme/assets/js/jquery-2.0.3.min.js'>" + "<" + "/script>");
    </script>

    <!-- <![endif]-->

    <!--[if IE]>
<script type="text/javascript">
 window.jQuery || document.write("<script src='~/Content/acetheme/assets/js/jquery-1.10.2.min.js'>"+"<"+"/script>");
</script>

<![endif]-->

    <script type="text/javascript">
        if ("ontouchend" in document) document.write("<script src='~/Content/acetheme/assets/js/jquery.mobile.custom.min.js'>" + "<" + "/script>");
    </script>

    <script src="~/Content/acetheme/assets/js/bootstrap.min.js"></script>
    <script src="~/Content/acetheme/assets/js/typeahead-bs2.min.js"></script>

    <!-- page specific plugin scripts -->
    @RenderSection("specificPluginScript", required: false)

    <script src="~/Content/acetheme/assets/js/jqGrid/jquery.jqGrid.min.js"></script>
	<script src="~/Content/acetheme/assets/js/jqGrid/i18n/grid.locale-en.js"></script>

    @Html.Partial("_loading")

    <!--[if lte IE 8]>
		  <script src="~/Content/acetheme/assets/js/excanvas.min.js"></script>
		<![endif]-->

    @*
    <script src="~/Content/acetheme/assets/js/jquery-ui-1.10.3.custom.min.js"></script>
    <script src="~/Content/acetheme/assets/js/jquery.ui.touch-punch.min.js"></script>
    <script src="~/Content/acetheme/assets/js/jquery.slimscroll.min.js"></script>
    <script src="~/Content/acetheme/assets/js/jquery.easy-pie-chart.min.js"></script>
    <script src="~/Content/acetheme/assets/js/jquery.sparkline.min.js"></script>
    <script src="~/Content/acetheme/assets/js/flot/jquery.flot.min.js"></script>
    <script src="~/Content/acetheme/assets/js/flot/jquery.flot.pie.min.js"></script>
    <script src="~/Content/acetheme/assets/js/flot/jquery.flot.resize.min.js"></script>
    *@

    <!-- ace scripts -->

    <script src="~/Content/acetheme/assets/js/ace-elements.min.js"></script>
    <script src="~/Content/acetheme/assets/js/ace.min.js"></script>

    <!-- inline scripts related to this page -->
    @RenderSection("Scripts", required: false)


</body>

</html>


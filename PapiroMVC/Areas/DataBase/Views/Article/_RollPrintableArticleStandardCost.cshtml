@using PapiroMVC.Validation
@model PapiroMVC.Models.RollPrintableArticleViewModel

@{
    string strings = "~/Areas/DataBase/Views/Article/Strings";
    ViewBag.Title = (string)HttpContext.GetLocalResourceObject(strings, "RollPrintableArticleStandardCostTitle");
}

    <fieldset>
            <legend>
                   @ViewBag.Title
                            </legend>
        @Html.ValidationMessageFor(model => model.RollPrintableArticleStandardCost)

        @Html.AlgolaEditorFor(model => model.RollPrintableArticleStandardCost.CostPerMq)
        @*Html.AlgolaEditorFor(model => model.RollPrintableArticleStandardCost.CostPerMl) *@

        @Html.HiddenFor(model => model.RollPrintableArticleStandardCost.TimeStampTable)
        @Html.HiddenFor(model => model.RollPrintableArticleStandardCost.CodArticleCost)
        @Html.HiddenFor(model => model.RollPrintableArticleStandardCost.CodArticle)

    </fieldset>

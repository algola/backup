@using PapiroMVC.Validation

@{
    string strings = "~/Areas/Account/Views/Account/Strings";
    ViewBag.Title = Html.T(strings,"ConfirmEditProfileTitle");
}

<hgroup class="title">
    <h1>@ViewBag.Title</h1>
    <h2>@Html.T(strings,"ConfirmEditProfile")</h2>
    <br />
    <p>
        <button type="button" class="default" onclick="Javascript:window.location.replace('@Url.Action("Index", "Home", new {Area=""})');">            
           <i class="icon-home"></i>
             @HttpContext.GetLocalResourceObject(strings, "Home")
        </button>
     </p>
</hgroup>
